{
  "name": "gesucuarana-website",
  "version": "1.0.0",
  "description": "Website do Grupo Escoteiro Suçuarana",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "install:all": "npm install && npm install --prefix frontend && npm install --prefix backend"
  },
  "keywords": [
    "escoteiros",
    "grupo escoteiro",
    "suçuarana"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^22.15.21",
    "@types/react-slick": "^0.23.13",
    "concurrently": "^8.2.2",
    "dotenv": "^16.5.0"
  },
  "dependencies": {
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1"
  }
}
